{
  "name": "EasyV",
  "version": "2.2.0",
  "main": "dist-electron/main/index.js",
  "description": "袋鼠云易知微数字孪生产品桌面版本",
  "author": "袋鼠云-EasyV <nianyi@dtstack.com>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run electron:generate-icons",
    "preview": "vite preview",
    "pree2e": "vite build --mode=test",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "e2e": "playwright test",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config",
    "electron:generate-icons": "electron-icon-builder --input=./resources/1024logo.png --output=public --flatten"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@lidakai/utils": "0.1.1-beta.1",
    "@material-ui/styles": "4.9.0",
    "adm-zip": "0.5.10",
    "ahooks": "3.7.8",
    "antd": "5.11.0",
    "async-retry": "^1.3.3",
    "axios": "^1.6.7",
    "csv-parse": "^5.5.3",
    "d3": "5.9.2",
    "electron-updater": "6.1.1",
    "fastq": "^1.16.0",
    "iconv-lite": "0.6.3",
    "jschardet": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mysql2": "^3.9.1",
    "qs": "^6.11.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.1",
    "recoil": "0.7.7",
    "three": "0.100.0",
    "typeorm": "0.3.17",
    "uuid": "9.0.1"
  },
  "config": {
    "electron": {
      "mirror": "https://registry.npmmirror.com/-/binary/electron/"
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "devDependencies": {
    "@easyv/linter": "^0.1.5",
    "@electron/rebuild": "3.3.1",
    "@playwright/test": "1.37.1",
    "@types/adm-zip": "0.5.4",
    "@types/antd": "^1.0.0",
    "@types/async-retry": "^1.4.8",
    "@types/axios": "^0.14.0",
    "@types/d3": "7.4.3",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.14",
    "@types/qs": "^6.9.11",
    "@types/react": "18.2.20",
    "@types/react-dom": "18.2.7",
    "@types/recoil": "^0.0.9",
    "@types/three": "0.159.0",
    "@types/unzip": "0.1.4",
    "@vitejs/plugin-react": "4.0.4",
    "asar": "^3.2.0",
    "autoprefixer": "10.4.16",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "3.2.9",
    "eslint": "8.56.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "npm-check": "^6.0.1",
    "postcss": "8.4.31",
    "reflect-metadata": "^0.2.1",
    "tailwindcss": "3.3.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-imp": "2.4.0"
  },
  "engines": {
    "node": "16.20.0 || >=18.0.0"
  }
}